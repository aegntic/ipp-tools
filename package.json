{
  "name": "ipp-tools-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:ipp": "vite --config vite.config.ipp.js",
    "dev:vibecascade": "vite --config vite.config.vibecascade.js",
    "build": "vite build",
    "preview": "vite preview",
    "build:ipp": "vite build --config vite.config.ipp.js",
    "build:vibecascade": "vite build --config vite.config.vibecascade.js",
    "build:all": "npm run build:ipp && npm run build:vibecascade",
    "deploy": "chmod +x ./scripts/deploy.sh && ./scripts/deploy.sh",
    "deploy:ipp": "chmod +x ./scripts/deploy.sh && ./scripts/deploy.sh ipp",
    "deploy:vibecascade": "chmod +x ./scripts/deploy.sh && ./scripts/deploy.sh vibecascade",
    "analyze": "source-map-explorer 'dist/**/*.js'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "framer-motion": "^10.16.5",
    "react-intersection-observer": "^9.5.3",
    "tailwindcss": "^3.3.5",
    "@headlessui/react": "^1.7.17",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "source-map-explorer": "^2.5.3",
    "vite": "^5.0.0"
  }
}